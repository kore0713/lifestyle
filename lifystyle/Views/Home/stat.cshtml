<!-- amCharts javascript code -->
<script type="text/javascript">
    AmCharts.makeChart("chartdiv",
        {
            "type": "pie",
            "balloonText": "[[title]]<br><span style='font-size:14px'><b>[[value]]</b> ([[percents]]%)</span>",
            "titleField": "category",
            "valueField": "column-1",
            "allLabels": [],
            "balloon": {},
            "legend": {
                "enabled": true,
                "align": "center",
                "markerType": "circle"
            },
            "titles": [],
            "dataProvider": [
                {
                    "category": "category 1",
                    "column-1": "5",
                    "column-2": 1
                },
                {
                    "category": "category 2",
                    "column-1": "5",
                    "column-2": 75
                },
                {
                    "category": "category 3",
                    "column-1": "5",
                    "column-2": 85
                },
                {
                    "category": "category 4",
                    "column-1": "5",
                    "column-2": null
                },
                {
                    "category": "category 5",
                    "column-1": "5",
                    "column-2": null
                }
            ]
        }
    );
</script>
